<h1>User list</h1>
<div>
  <ul>
    <li><%= link_to "New", new_user_path %></li>
  </ul>
</div>

<table border="0">
  <tr>
    <th>email</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Admin</th>
    <th></th>
  </tr>
  <% for user in @users %>
  <tr>
    <td><%= user.email %></td>
    <td><%= user.first_name %></td>
    <td><%= user.last_name %></td>
    <td><%= user.admin ? "Yes" : "No" %> </td>
    <td><%= link_to "edit", edit_user_path(user) %> </td>
  </tr>
  <% end %>

</table>