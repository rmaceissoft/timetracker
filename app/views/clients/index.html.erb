<div>
  <ul>
    <li><%= link_to "New", new_client_path %></li>
  </ul>
</div>

<table border="0">
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Initials</th>
    <th>Company Name</th>
    <th>Email</th>
    <th>Phone</th>
    <th>City</th>
    <th>State</th>
    <th>Zip Code</th>
    <th>Country</th>
    <th>Active</th>
    <th></th>
  </tr>
  <% @clients.each do |client| %>
  <tr>
    <td><%= client.first_name %></td>
    <td><%= client.last_name %></td>
    <td><%= client.initials %></td>
    <td><%= client.company_name %></td>
    <td><%= client.email %></td>
    <td><%= client.phone %></td>
    <td><%= client.city %></td>
    <td><%= client.state %></td>
    <td><%= client.zip_code %></td>
    <td><%= client.country %></td>
    <td><%= client.active ? "Yes" : "No" %></td>
    <td><%= link_to "edit", edit_client_path(client) %></td>
  </tr>
  <% end %>

</table>
