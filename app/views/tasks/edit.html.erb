<%= semantic_form_for @task do |f| %>
  <%= f.inputs do %>
    <%= f.input :proyect, :include_blank => "Select proyect" %>
    <%= f.input :description %>
    <%= f.input :hours, :as => :select, :collection => Task.get_hours_options, :include_blank => false %>
    <%= f.input :is_billable %>
    <%= f.input :date_add %>
  <% end %>
  <%= f.actions %>
<% end %>
